    <fields>
        <fullName>sf_devops__State__c</fullName>
        <deprecated>false</deprecated>
        <description>The current state of this Work Item record.</description>
        <externalId>false</externalId>
        <formula>IF(
  sf_devops__Concluded__c = "CLOSED", "CLOSED",
IF(
  sf_devops__Concluded__c = "NEVERED", "NEVERED",
  IF(
    sf_devops__Concluded__c = "COMBINED", "COMBINED",
  IF(
    sf_devops__Promoted__c, "PROMOTED",
    IF(
      sf_devops__Development_Approved__c, "APPROVED",
      IF(
        NOT(ISBLANK(sf_devops__Review_Remote_Reference__c)), "IN_REVIEW",
        IF(
          NOT(ISBLANK(sf_devops__Branch__r.sf_devops__Name__c)), "IN_PROGRESS", "NEW"
          )
        )
      )
    )
  )
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The current state of this Work Item record.</inlineHelpText>
        <label>Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
